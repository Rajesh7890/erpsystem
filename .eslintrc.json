{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parser": "babel-eslint",
	"extends": [
		  "airbnb",
	    "eslint:recommended",
	    "plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true,
			"arrowFunctions": true,
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"semi": ["error", "always"]
		// "quotes": ["error", "double"]
	}
}
